<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of Your Cars</title>
</head>
<body>
    <h1>List of Your Cars</h1>
    <% list.forEach(function(car) { %>
    <div>
        <h2>
            <a href="/cars/<%=car.id%>"><%= car.name %></a> 
        </h2>
        <p><%= car.typeOfVehicle %></p>
    </div>
    <% }) %>
    <a href="/">Return to Homepage</a>
</body>
</html>